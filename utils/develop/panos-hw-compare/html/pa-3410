                PAN.RunOnPageModelLoad = [];
				window.PAN_RunOnPageModelLoad = [];
                

                $(document).ready(function() {
                    function loadAsyncScript(uri) {
                        return new Promise(function(resolve, reject) {
                            var tag = document.createElement('script');
                            tag.src = uri;
                            tag.async = true;
                            tag.onload = function() {
                                resolve();
                            }
                            //var firstScriptTag = document.getElementsByTagName('script')[0];
                            //firstScriptTag.parentNode.insertBefore(tag, firstScriptTag);
                            document.getElementsByTagName('body')[0].appendChild(tag);
                        });
                    }
                    var scriptLoaded = loadAsyncScript('/apps/pan/public/singlePageReactModel?pageId=/content/pan/en_US/products/product-comparison');
                    scriptLoaded.then(function() {
                        var i=0;
                        try {
                            while (i<PAN.RunOnPageModelLoad.length) {
                                try { PAN.RunOnPageModelLoad[i](); }
                                catch (e) {}
                                i++;
                            }
                        } catch (e2) {}
						i=0;
						try {
							while (i<PAN_RunOnPageModelLoad.length) {
								try { PAN_RunOnPageModelLoad[i](); }
								catch (e) {}
								i++;
							}
						} catch (e2) {}
                    });
                });
				</script>
