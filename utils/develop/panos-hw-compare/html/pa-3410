<script src="/etc/clientlibs/clientlibs_prodcomp.min.js"></script>
<script type="text/javascript">
        var Manager;
        var trt = 'pan/components/custom/productcompare/productcomparecontroller';
        var corename = 'productcompare';

        (function ($) {

          $(function () {
            Manager = new AjaxSolr.Manager({
              
                  proxyUrl: '/apps/pan/public/solr/proxy'
                
            });

            

    AjaxSolr.ResultWidget = AjaxSolr.AbstractWidget.extend({
      start: 0,

      beforeRequest: function () {
        $(this.target).html($('<img>').attr('src', '/etc/clientlibs/pan/img/spinner-bg.svg'));
      },

      facetLinks: function (facet_field, facet_values) {
        var links = [];
        if (facet_values) {
          for (var i = 0, l = facet_values.length; i < l; i++) {
            if (facet_values[i] !== undefined) {
              links.push(
                $('<a href="#"></a>')
                .text(facet_values[i])
                .click(this.facetHandler(facet_field, facet_values[i]))
              );
            }
            else {
              links.push('no items found in current selection');
            }
          }
        }
        return links;
      },

      facetHandler: function (facet_field, facet_value) {
        var self = this;
        return function () {
          self.manager.store.remove('fq');
          self.manager.store.addByValue('fq', facet_field + ':' + AjaxSolr.Parameter.escapeValue(facet_value));
          self.doRequest();
          return false;
        };
      },

      afterRequest: function () {
      	var products = '';
        $(this.target).empty();
        var docs = this.manager.response.response.docs;
        var docCount = docs.length;
        var dfi = '_dfi';
        
        if(docCount > 0)
        {
        	var chosenKeys = Object.keys(chosen);
			var output = '';
			output += '<thead><tr><th>Feature</th>';
			for(var i = 0, l = docs.length; i < l; i++)
			{
				//var chosenId = chosenKeys[i];
				//var doc = getDocById(docs, chosenId);
				//if(!doc)
				//{
				//	continue;
				//}
				doc = docs[i];
				var smallThumbnailUrl = doc['small-image-url'];
                smallThumbnailUrl = smallThumbnailUrl.replace("/jcr:content/renditions/cq5dam.thumbnail.72.72.margin.png", ".transform/productThumbSmall/image.png");
				output += '<th class="prod-col"><h2><a href="' + doc['url'] + '">' + doc['product_name'] + '</a></h2>';
				output += '<img class="lozad" alt="' + doc['product_name'] + '" data-src="' + smallThumbnailUrl + '">';
				output += '</img>';
				output += '<p><a data-pid="' + doc['product_id'] + '" class="remove" title="' + doc['product_name'] + '">Remove</a></p>'
				output += '</th>';
				products += doc['category'] + ';' + doc['product_id'];
		          if(i < l - 1)
		          {
		          	products += ',';
		          }
			}


			output += '</tr></thead><tbody>';
			var currentGroup = "";
			for(var k in featureMap)
			{
				if(k.toLowerCase().indexOf("facet") == -1)
				{
	          		if({}.hasOwnProperty.call(featureMap, k))
	          		{
	          			var f = featureMap[k];
                        var fname = f['name'];
						if(fname && typeof fname !== 'undefined' && fname.toLowerCase().indexOf("facet") == -1)
                        {
                            var featureDetails = '';
                            if({}.hasOwnProperty.call(f, 'details'))
                            {
                                //featureDetails = f['details'];
                                featureDetails = '<a class="footnote" href="" data-toggle="tooltip" data-placement="right" title="' + f['details'] + '">*</a><span class="footnote-text">* ' + f['details'] + '</span>';
                            }
                            var g = f['group'];
                            var gd = g;
                            if(g != currentGroup)
                            {
                                if({}.hasOwnProperty.call(groupMap, g))
                                {
                                    var groupObj = groupMap[g];
                                    if({}.hasOwnProperty.call(groupObj, 'details'))
                                    {
                                        var groupDetails = groupObj['details'];
                                        gd+= '<a class="footnote" href="" data-toggle="tooltip" data-placement="right" title="' + groupDetails + '">*</a><span class="footnote-text">* ' + groupDetails + '</span>';
                                    }
                                }
                                output += '<tr><td colspan="' + (l + 1) + '" class="prod-cat">' + gd + '</td></tr>';
                                currentGroup = g;
                            }
                            output += '<tr><td class="prod-spec">'+ f['name'] + featureDetails + '</td>';
                            for(var i = 0, l = docs.length; i < l; i++)
                            {
                                //var chosenId = chosenKeys[i];
                                //var doc = getDocById(docs, chosenId);
                                //if(!doc)
                                //{
                                //	continue;
                                //}
                                doc = docs[i];
                                var langlessId = doc.id.substring(doc.id.indexOf('-') + 1);
                                var productNotes = null;
                                if({}.hasOwnProperty.call(f, 'productNotes'))
                                {
                                    var productNotesObj = f['productNotes'];
                                    if({}.hasOwnProperty.call(productNotesObj, langlessId))
                                    {
                                        productNotes = productNotesObj[langlessId];
                                    }
                                }
                                if({}.hasOwnProperty.call(doc, k))
                                {
                                    output += '<td>' + doc[k]
                                    if(productNotes)
                                    {
                                        output += '<a class="footnote" href="" data-toggle="tooltip" data-placement="right" title="' + productNotes['note'] + '">*</a><span class="footnote-text">* ' + productNotes['note'] + '</span>';
                                    }
                                    output += '</td>';
                                }
                                else
                                {
                                    output += '<td>N/A</td>';
                                }
                            }
                            output += '</tr>';
                    	}
	          		}
          		}
          	}
          	output += '</tbody>';
          	$(this.target).append(output);
        }
        
        if(typeof s != 'undefined')
        {
        	console.log("Found SiteCatalyst");
	        s.products = products;
	        s.events = 'prodView,event1';
	        s.linkTrackVars='events,products';
	    	s.linkTrackEvents='prodView,event1';
	        s.tl(true, 'o', 'Product View');
        }
        else
        {
        	console.log("Could not find SiteCatalyst");
        }
        initTooltip();
        initChosen();
        initRemove();
        initPdfButton();
        
        //for (var i = 0, l = this.manager.response.response.docs.length; i < l; i++) {
        //  var doc = this.manager.response.response.docs[i];
        //  $(this.target).append(this.template(doc));
        //}
      },

      template: function (doc) {
        var snippet = '';
        if(doc.teaser.length > 250)
        {
        	snippet += doc.teaser.substring(0, 250);
        	snippet += '<span style="display:none">' + doc.teaser.substring(250);
        	snippet += '</span> <a href="#" class="more">more</a>';
        }
        else
        {
        	snippet += doc.teaser;
        }

        var title = doc.title;
        var output = '';

        if(this.manager.response.highlighting && this.manager.response.highlighting[doc.id]) {
            if(this.manager.response.highlighting[doc.id]['teaser']) {
                var teaserSnippet = this.manager.response.highlighting[doc.id]['teaser'][0];

                if (teaserSnippet.length > 250) {
                    snippet = teaserSnippet.substring(0, 250);
                    snippet += '<span style="display:none">' + teaserSnippet.substring(250);
                    snippet += '</span> <a href="#" class="more">more</a>';
                } else {
                    snippet = teaserSnippet;
                }
            }
        }

        //output += '<div class="result-card">';
        //output += '<a href="' + doc['product_id'] + '" class="google-title">' + title + '</a>';
        //output += '<div class="google-url">' + doc['product_id'] + '</div>';
        //output += '<div>' + snippet + '</div>';
        //output += '<table>';
        //for(var k in doc)
        //{
        //	if({}.hasOwnProperty.call(doc, k))
        //	{
        //		output += '<tr><td>'+ k + '</td><td>' + doc[k] + '</td></tr>';
        //	}
        //}
        //output += '</table>';
        //output += '</div>';


        return output;
      },

      init: function () {
        $(document).on('click', 'a.more', function () {
          var $this = $(this),
              span = $this.parent().find('span');

          if (span.is(':visible')) {
            span.hide();
            $this.text('more');
          }
          else {
            span.show();
            $this.text('less');
          }

          return false;
        });
      }
    });

    Manager.addWidget(new AjaxSolr.ResultWidget({
      id: 'result',
      target: '#searchresults'
    }));


            Manager.init();

            
              Manager.store.addByValue('corename', 'productcompare');
            

			var chosenQP = getChosen();
			if(!chosenQP)
			{
				Manager.store.addByValue('q', '*:*');
			}
			else
			{
                console.log(paramToQueryString(chosenQP));
				Manager.store.addByValue('q', paramToQueryString(chosenQP));
			}

			var passFq = 'language:"en_US"' ;
            

            var params = {
              
              'fq': passFq,

              'fl': '',
              
              'rows': 10,
              'json.nl': 'map',
              'sort': 'position asc'
            };
            for (var name in params) {
              Manager.store.addByValue(name, params[name]);
            }
                Manager.doRequest();
              });

            })(jQuery);
        </script>

